{
  "name": "rockbuzz/lara-posts",
  "description": "Management of posts",
  "keywords": ["laravel", "blog", "posts", "articles"],
  "license": "MIT",
  "authors": [
    {
      "name": "TiagoDevWeb",
      "email": "tiago@rockbuzz.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rockbuzz\\LaraPosts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "rockbuzz/lara-uuid": "^2.0",
    "spatie/laravel-sluggable": "^2.1",
    "bensampo/laravel-enum": "^2.0",
    "spatie/laravel-schemaless-attributes": "~1.8",
    "spatie/eloquent-sortable": "^3.11"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/phpcs"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rockbuzz\\LaraPosts\\ServiceProvider"
      ]
    }
  }
}
